{
  "private": true,
  "scripts": {
    "start": "cross-env API_ENV=dev umi dev",
    "build": "cross-env API_ENV=prod umi build",
    "build:test": "cross-env API_ENV=test umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.26.4",
    "dayjs": "^1.10.7",
    "qs": "^6.10.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "cross-env": "^7.0.3",
    "lint-staged": "^11.2.3",
    "prettier": "^2.2.0",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
